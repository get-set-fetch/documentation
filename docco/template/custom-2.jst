<div class="col container">
    <% for (var i=1, l=sections.length; i<l; i++) { %>
    <% var section = sections[i]; %>
    <div class="row">
        <div class="col-6 col-comment fixed">
            <%= section.docsHtml %>
        </div>
        <div class="col-6 col-source">
{% highlight javascript %}
<%= section.codeText %>
{% endhighlight %}
        </div>
    </div>
    <% } %>
</div>
